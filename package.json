{
  "name": "simgr",
  "description": "Image resizing proxy using S3 as a backend",
  "version": "4.0.0",
  "author": {
    "name": "Jonathan Ong",
    "email": "me@jongleberry.com",
    "url": "http://jongleberry.com",
    "twitter": "https://twitter.com/jongleberry"
  },
  "license": "MIT",
  "repository": "mgmtio/simgr",
  "dependencies": {
    "debug": "*",
    "fs-cp": "1",
    "fs-reap": "1",
    "hash-stream": "1",
    "http-errors": "1",
    "knox": "0",
    "mime-types": "2",
    "mkdirp": "0",
    "mz": "1",
    "native-or-bluebird": "1",
    "phash-image": "2",
    "sharp": "0",
    "type-is": "1"
  },
  "devDependencies": {
    "bluebird": "2",
    "fluent-ffmpeg": "*",
    "istanbul-harmony": "0",
    "mocha": "2",
    "rimraf": "2"
  },
  "scripts": {
    "test": "mocha --harmony-generators --reporter spec test/index.js --bail",
    "test-cov": "node --harmony-generators ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- test/index.js",
    "test-travis": "node --harmony-generators ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- test/index.js"
  },
  "main": "lib",
  "files": [
    "lib"
  ],
  "keywords": [
    "image",
    "resize"
  ]
}
