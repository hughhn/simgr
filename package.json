{
  "name": "simgr",
  "description": "Image resizing proxy using S3 as a backend",
  "version": "5.1.0",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "mgmtio/simgr",
  "dependencies": {
    "debug": "*",
    "fs-lru-cache": "^1.0.2",
    "hash-stream": "1",
    "http-errors": "1",
    "knox": "0",
    "mime-types": "2",
    "mkdirp": "0",
    "mz": "1",
    "sharp": "0",
    "type-is": "1"
  },
  "devDependencies": {
    "fluent-ffmpeg": "*",
    "istanbul": "0",
    "mocha": "2",
    "rimraf": "2"
